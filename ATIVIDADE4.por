programa
{
	
	funcao inicio()
	{
		cadeia nomeGringo
		real temperaturaC
		real temperaturaF

		escreva("Gringo, qual e o seu nome : ")
		leia(nomeGringo)
		escreva("qual a temperatura em Celcius : ")
		leia(temperaturaC)
		temperaturaF = (temperaturaC * 9/5)+32
		escreva("oi "+nomeGringo+" a temperatura em farren é "+temperaturaF+"F")
	}
}
